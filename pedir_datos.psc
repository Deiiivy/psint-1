Algoritmo pedir_datos
	Definir nombre, apellidos, genero Como caracter;
	Definir cedula, edad Como Entero;
	Escribir "ingrese nombre"
	Leer nombre
	Escribir "ingrese apellidos"
	Leer apellidos
	Escribir "ingrese genero"
	Leer genero
	Escribir "ingrese su cedula"
	Leer cedula
	Escribir "ingrese su edad"
	Leer edad
	Escribir "nombre: ", nombre
	escribir "apellidos: ", apellidos
	Escribir "genero: ", genero
	Escribir "cedula: ", cedula
	escribir "edad: ", edad
FinAlgoritmo
